pipeline {
agent any
tools {nodejs "node"}
stages {
  stage('checkout scm') { 
  steps{
    checkout([$class: 'GitSCM', branches: [[name: '**']], doGenerateSubmoduleConfigurations: false, extensions: [], submoduleCfg: [], userRemoteConfigs: [[credentialsId: 'githubcred', url: 'https://github.com/rxa80330/Nodejs-application.git']]])
  }
  }
  
     stage('Build Project') {
      steps{
              bat'npm install'
               bat 'npm run build'
                 } 
            }
}
}
