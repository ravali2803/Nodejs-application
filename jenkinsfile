pipeline {
agent any
tools {nodejs "node"}
stages {
  stage('checkout scm') { 
  steps{
    git branch: 'main', credentialsId: 'githubcred', url: 'https://github.com/rxa80330/Nodejs-application.git'
  }
  }
  
     stage('Build Project') {
      steps{
              bat'npm install'
               bat 'npm run build'
                 } 
            }
}
}
