pipeline{
agent any    
stages {
  stage('checkout scm') { 
   checkout([$class: 'GitSCM', doGenerateSubmoduleConfigurations: false, extensions: [], submoduleCfg: [], userRemoteConfigs: [[url: 'https://github.com/rxa80330/Nodejs-application.git']]])
  }
     stage('Build Project') {
              bat ''' 'npm install'
                  'npm run build' '''
            }
}
}
